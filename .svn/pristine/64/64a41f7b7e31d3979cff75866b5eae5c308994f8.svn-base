.crenoVide{
	border-collapse: collapse;
	border-bottom:dotted 1px #C1D2EE;
	width:100%;
}

.crenoPlein{
color:black;
border-collapse: collapse;
/*border:solid 1px #888888;*/
border:solid 1px #225588;
font-size:11px;
text-align:center;
}

a {
color:black;
text-decoration:none;
}

.crenoPlein div{
display: table-cell;
vertical-align: middle;
text-align:center;
width:100%;
height:100%;
}


.globalTable {
	/*border:solid 2px black;*/
	color: Black;
	font-family: "Lucida Grande", Verdana, Tahoma, Arial, Times;
	font-size: 13px;
	border:0;
}

.globalTable td {
	padding:2px;
}

.globalTable th {
	background-color:#E9EDF2;
	color:#225588;
	padding:2px;
}


.timeline {
	border-right:solid 1px #AAAAAA;
	background-color:#E9EDF2;
	color:#225588;
}

.timeline div {

	color: Black;
	font-family: "Lucida Grande", Verdana, Tahoma, Arial, Times;
	font-size: 13px;
border-bottom:solid 1px#C1D2EE;
width:50px;
}


.jourField{
border-right:solid 1px #AAAAAA;
border-right:3px double #C1D2EE;
}

.tableinner {
padding:0px;
margin:0px;
