	border-spacing: 5px; /*cellspacing:poor IE support for  this*/
	/* border-collapse:separate;*/
}

.divRow {
	display: table-row;
	width: auto;
}

.divRowSearch:nth-child(even) {
	display: table-row;
	width: auto;
	background-color: #F9F9F9;
	background-color: #f3f4f8;
}

.divRowSearch:nth-child(odd) {
	display: table-row;
	width: auto;
	background-color: none;
}

.divCellSearch {
	vertical-align: top ;
	padding: 0.2em;
	float: left; /*fix for  buggy browsers*/
	display: table-column;
	width: 60%;
	/*display: inline;*/
}

.divCell {
	float: left; /*fix for  buggy browsers*/
	display: table-column;
	width: auto;
	/*background-color: #ccc;*/
}

fieldset {
	/*border:#993366 1px solid;*/
	border: #CDCFCF 1px solid;
	margin: 1em 0em 1em 0em;
	padding: 0em 1em 1em 1em;
}

.actions {
	height: 24px;
	display: block;
	padding-top: 4px;
	background-color: #e9edf2;
	/*#F9F9F9;	*/
	text-align: right;
	border: #c1d2ee 1px solid;
}

