	where categ_id = cd.categ_id) as categ_lib,
 ct.langue_$c as LANGUE,
 ct.titre,
 ct.objectif,
 ct.programme,
 ct.cre_date,
 ct.modif_date,
 ct.maj_auteur 
from 
	cata_trad ct,
	catalogue ca,
	cata_dipl cd
where
 ct.cata_id = ca.cata_id
 and ca.cata_id = cd.cata_id
 and ca.an_univ = $P{ANNEE}
order by categ_id,uv
