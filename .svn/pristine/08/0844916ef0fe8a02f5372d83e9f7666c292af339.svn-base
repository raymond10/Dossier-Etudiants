	protected void testResults(WODisplayGroup dg) {
		if (dg.allObjects().count() == 0) {
			wocomponent.setFoundResults(false);
			wocomponent.setNoResult(true);
		} else {
			wocomponent.setNoResult(false);
			wocomponent.setFoundResults(true);
		}
	}

