	protected void testResults(WODisplayGroup dg) {
		if (dg.allObjects().count() == 0) {
			wocomponent().setFoundResults(Boolean.FALSE);
			wocomponent().setNoResult(Boolean.TRUE);
		} else {
			wocomponent().setNoResult(Boolean.FALSE);
			wocomponent().setFoundResults(Boolean.TRUE);
		}
	}

