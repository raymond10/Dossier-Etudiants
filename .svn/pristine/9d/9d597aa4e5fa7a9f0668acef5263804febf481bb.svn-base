//icon
shortcut_icon : WOGenericElement {
	elementName = "link";
	href = icon;
	type = "image/x-icon";
	rel = "shortcut icon";
}

icon : WOGenericElement {
	elementName = "link";
	href = icon;
	type = "image/x-icon";
	rel = "icon";
}

//Style CSS
LinkCssPrint : WOGenericElement {
	elementName = "link";
	rel = "stylesheet";
	href = cssPrint;
	type = "text/css";
	media = "print";
}

LinkCssEdt : WOGenericElement {
	elementName = "link";
	rel = "stylesheet";
	href = cssEdt;
	type = "text/css";
	media = "screen, projection";
	title = "Normal";
}

LinkCssTip : WOGenericElement {
	elementName = "link";
	rel = "stylesheet";
	href = cssTip;
	type = "text/css";
	media = "screen, projection";
}

LinkCssCalendar_blue : WOGenericElement {
	elementName = "link";
	rel = "stylesheet";
	href = cssCalendarBlue;
	type = "text/css";
	media = "screen, projection";
}

