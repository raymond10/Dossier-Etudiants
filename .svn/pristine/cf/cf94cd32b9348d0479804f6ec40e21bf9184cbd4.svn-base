	margin-left: 10px;
	background: white;
}

#res-menu a {
	text-decoration: none;
	display: block;
	border-bottom: #eee 1px solid;
	background: white;
	color: #777;
	padding: 0 7px;
	margin: 0;
	zoom: 1;
	z-index: 10;
}

#res-menu a:hover,#res-menu .active {
